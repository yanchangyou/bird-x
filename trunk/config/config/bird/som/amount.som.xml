<?xml version="1.0"?>

<som>
	<class name="test.Amount" 
			sheet="金额" titleRowIndex="1"
		firstDataRowIndex="2">
		<property name="amount" title="金额">
			<string_rule>
				<description>金额规则</description>
				<match>^\d{1,17}(\.\d{0,2})?$</match>
				<emptyable>false</emptyable>
				<message>请输入正确的[金额]</message>
			</string_rule>
		</property>
	</class>
</som>