<?xml version="1.0" encoding="UTF-8"?>
<all>
	<message uri="excel">
		<error_messge></error_messge>
		<warning_message></warning_message>
	</message>

	<type>
		<not_exist_excel></not_exist_excel>
		<exist_excel>
		
			<not_exist_sheet_by_name></not_exist_sheet_by_name>
		</exist_excel>
	</type>
	
	
	<input context="page">
		<type>文件</type>
		<limit>后缀名为xsl的文件</limit>
		<case-in-limit>
			<case index="1"></case>
		</case-in-limit>
	</input>
	
	<input context="action">
		<type>文件</type>
		<limit>后缀名为xsl的文件</limit>
		<case-in-limit>
			<case index="1"></case>
		</case-in-limit>
	</input>


	<SOM完整性检查>
		<check index="1">SOM所有的类必须存在</check>
		<check index="2">som所有配置文件必须存在</check>
		<check index="3">string_rule所有配置文件必须存在</check>
	</SOM完整性检查>
	
	<模板文件完整性检查>
		<check index="1">文件必须存在</check>
		<check index="2">文件名必须存在</check>
		<check index="3">文件后缀名必须是xls</check>
		<check index="4">文件内容必须存在</check>
		<check index="5">文件内容必须是excel文件内容</check>
		<check index="6">配置文件中的sheet存在</check>
		<check index="7">配置文件中sheet的title必须存在</check>
		<check index="8">配置文件中sheet的title必须在第一行, 如果配置文件有title位置的属性就必须是配置文件中指定的行位置</check>
	</模板文件完整性检查>
	<all-judge>
		<judge index="1">文件必须存在</judge>
		<judge index="2">文件后缀名必须是xls</judge>
		<judge index="3">文件内容是excel文件内容</judge>
		<judge index="4">配置文件中的sheet存在</judge>
		<judge index="5">配置文件中sheet的title必须存在</judge>
		<judge index="6">配置文件中sheet的title必须存在</judge>
	</all-judge>

	<all-judge>
		<need-judge index="1">文件存在</need-judge>
		<need-judge index="2">文件后缀名是xls</need-judge>
		<need-judge index="3">文件中开头几个字节是D0, CF, 11, EO, A1, B1, 1A, E1</need-judge>
		<not-need-judge index="4">存在workbook(excel本身已经校验)</not-need-judge>
		<not-need-judge index="4">存在workbook(excel本身已经校验)</not-need-judge>
		<not-need-judge index="5">存在sheet(excel本身已经校验)</not-need-judge>
		<need-judge index="5">配置文件中的sheet存在</need-judge>
		<need-judge index="5">配置文件中的sheet存在</need-judge>
	</all-judge>
</all>
